{
    "name": "IoC-experiments",
    "version": "1.0.0",
    "description": "Proving ground for IoC + JS ideas",
    "main": "src/main.js",
    "scripts": {
        "start": "node src/index.js",
        "debug": "node --inspect-brk src/index.js",
